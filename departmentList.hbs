<div class="row">
    <div class="col-md-12">
        <h1>{{title}}<h1>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-md-12">
         <div calss="table-responsive">  
            <table class="table">
                 <thead>
                    <tr>
                        <th>Department Number</th>>
                        <th>Department Name</th>>
                    </tr>
                </thead>
                 <tbody>
                    {{#each data}}
                    <tr>
                        <td>{{departmentId}}</td>
                        <td>{{departmentName}}</td>
                    </tr>
                    {{else}}
                    <tr>
                        <td colspan="8">No Data is Available</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
    </div>
</div>
